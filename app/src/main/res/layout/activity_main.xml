<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kr.ac.kw.coms.globealbum.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="start map"
            android:onClick="startOnClick"
            android:id="@+id/btn_start_map"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="start web gallery"
            android:onClick="startOnClick"
            android:id="@+id/btn_start_web_gallery"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="start storage"
            android:onClick="startOnClick"
            android:id="@+id/btn_start_storage"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Open Navigator"
            android:onClick="startOnClick"
            android:id="@+id/btn_start_navigator"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Open Diary"
            android:onClick="startOnClick"
            android:id="@+id/btn_start_diary"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="start Login"
            android:onClick="startLogin"
            android:id="@+id/btn_start_login"
            />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
